  </Reference>
 </ItemGroup>

 <ItemGroup>
  <None Update="Properties\launchSettings.json">
   <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
  </None>
 </ItemGroup>

</Project>